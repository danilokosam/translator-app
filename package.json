{
  "name": "translator-app-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
